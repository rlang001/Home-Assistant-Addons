git init
git add rtl_cmd
git commit -m "first commit"
git remote add origin git@github.com:rlang001/Home-Assistant-Addons.git
git config core.sshCommand "ssh -i /config/keys/github/id_rsa -F /dev/null"

# Copy key to github

git push -u origin main
